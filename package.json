{
  "name": "Amway",
  "version": "0.0.0",
  "description": "Amway Frontend building package",
  "author": "EPAM Systems Ukraine",
  "private": true,
  "scripts": {
    "test": "jest",
    "postinstall": "webpack"
  },
  "engines": {
    "node": "4.2.6",
    "npm": "2.14.12"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/src/mocks/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/src/mocks/fileMock.js",
      "helpers": "<rootDir>/src/helpers",
      "components": "<rootDir>/src/components",
      "constants": "<rootDir>/src/constants/constants.js"
    },
    "modulePaths": [],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "testRegex": "__tests__/.*test.js$"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-core": "6.11.4",
    "babel-jest": "14.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-runtime": "6.11.6",
    "bootstrap-sass": "3.3.7",
    "css-loader": "0.23.1",
    "es6-promise": "3.2.1",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "9.0.1",
    "eslint-import-resolver-webpack": "0.4.0",
    "eslint-loader": "1.4.1",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "1.2.0",
    "eslint-plugin-react": "5.2.2",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "jest-cli": "14.0.0",
    "jquery": "3.1.0",
    "keymirror": "^0.1.1",
    "lodash": "4.14.0",
    "node-sass": "3.8.0",
    "postcss-loader": "0.9.1",
    "resolve-url-loader": "1.6.0",
    "sass-loader": "4.0.0",
    "sasslint-loader": "0.0.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "whatwg-fetch": "1.0.0"
  }
}
